fun foo(a: Int) : Int {
    var b = 1 + a
    return b
}
fun fao(a: String) {
    print(a)
}

fun bao(alpha : Int, betta : Int): Int = alpha + betta

fun main() {
    // first
    val a = 1
    var b = foo(a)
    print(fao("abc"))
    // second
    val str = "bebebe"
	print(fao(str))

    // third
    var alpha = 10
    var betta = 5

    print(bao(alpha, betta))

    b = 3
}