fun main() {
    val x: Int = 5
    var y: Real = 3.14
    val s: String = "Hello"
    var flag: Boolean = true
    val unitVal: Unit = Unit

    if (x > 0 && flag || !false) {
        print("positive\n")
    } else {
        print("negative\n")
    }

    when (x) {
        1 -> print("one\n")
        2, 3 -> print("two or three\n")
        in 4..10 -> print("from 4 to 10\n")
        !in 11..20 -> print("not in 11..20\n")
        else -> print("other\n")
    }

    for (i in 1..10 step 2) {
        print(i)
        print(",")
    }
    print("\n")

    for (j in 5 downTo 1) {
        print(j)
        print(";")
    }
    print("\n")

    var k = 0
    do {
        k = k + 1
    } while (k < 3)

    val res: Int = myFun(2, 3)
    print("myFun = ")
    print(res)
    print("\n")

    for (i in 1..10) {
        if (i == 3) continue
        if (i == 7) break
        print(i)
        print(" ")
    }
    print("\n")

    if (s is String) {
        print("s is String\n")
    }

    print("Enter a number: ")
    val input: String = readLine()
    print("You entered: ")
    print(input)
    print("\n")

    var a: Int = 2 + 3 - 1
    a = a * 4 / 2 % 3
    val exp: Int = 2 ** 3
    print("exp = ")
    print(exp)
    print("\n")

    if (a == 1 || a != 2 || a < 3 || a > 0 || a <= 5 || a >= -1) {
        print("rel_ops work\n")
    }

    val quoted: String = "quoted"
    val backtick: Int = `fun`()
    print("backtick = ")
    print(backtick)
    print("\n")

    val maybe: String = "some"
    val safe: String = maybe
    print("safe = ")
    print(safe)
    print("\n")

    print("All tokens tested!\n")
}

fun myFun(a: Int, b: Int): Int {
    return a + b
}

fun `fun`(): Int {
    return 42
}