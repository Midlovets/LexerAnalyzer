	15	int
	=	assign_op
	message	l-val
	"Hello, Valeriia"	string
	=	assign_op
	message	r-val
	OUT	out_op
	message	l-val
	"Hello, world!"	string
	=	assign_op
	message	r-val
	OUT	out_op
	a	l-val
	2.0	float
	1.0	float
	+	math_op
	5.0	float
	+	math_op
